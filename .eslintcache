il/Detail.jsx",["197","198","199","200"],"import ProductDetail from \"./ProductDetail\";\nimport Loading from \"../../global/Loading/Loading\";\nimport { useEffect, useState, useContext } from \"react\";\nimport { useParams, useHistory } from \"react-router-dom\";\nimport { Store } from \"../../../store/CartContext\";\nimport { StoreProduct } from \"../../../store/dbContext\";\n\nconst Detail = () => {\n  const [producto, setProducto] = useState({});\n  const [productos, setProductos] = useContext(StoreProduct);\n  const [data] = useContext(Store);\n  const history = useHistory();\n  const { idProducto } = useParams();\n\n  const handleRedirect = () => {\n    history.push(\"/cart\");\n  };\n  const getProductById = () => {\n    if (!!productos) {\n      let productoFiltrado = productos.filter((item) => item.id == idProducto);\n      setProducto(productoFiltrado[0]);\n    }\n  };\n  //para mostrar la cantidad ya agregada al count\n  const cantAmount = data.items.find(({ id }) => id === idProducto);\n  useEffect(() => {\n    getProductById();\n  }, []);\n  return (\n    <>\n      {productos ? (\n        <ProductDetail\n          producto={producto}\n          handleRedirect={handleRedirect}\n          //amount={cantAmount?.amount}\n        />\n      ) : (\n        <Loading />\n      )}\n    </>\n  );\n};\nexport default Detail;\n",["201","202"],"/home/gaston/Documentos/CoderHouseReact/coder-tienda/src/db/config.js",[],"/home/gaston/Documentos/CoderHouseReact/coder-tienda/src/components/global/Checkout/SaleConfirmed.jsx",[],"/home/gaston/Documentos/CoderHouseReact/coder-tienda/src/components/global/Navbar/Style.jsx",[],"/home/gaston/Documentos/CoderHouseReact/coder-tienda/src/components/global/Checkout/FormCheckout.jsx",[],["203","204"],"/home/gaston/Documentos/CoderHouseReact/coder-tienda/src/components/global/Navbar/ButtonMenu.jsx",[],"/home/gaston/Documentos/CoderHouseReact/coder-tienda/src/components/global/ItemListContainer/ItemListContainer.jsx",["205"],"import ItemList from \"./ItemList\";\nimport Loading from \"./../Loading/Loading\";\nimport { useState, useEffect, useContext } from \"react\";\nimport { StoreProduct } from \"../../../store/dbContext\";\n\nconst ItemListContainer = ({ categoria }) => {\n  const [items, setItems] = useState({});\n\n  const [productos] = useContext(StoreProduct);\n\n  const getProductsFromDB = () => {\n    if (productos) {\n      if (!!categoria) {\n        let arrFiltrado = productos.filter(\n          (item) => item.data.categoria === categoria\n        );\n\n        setItems(arrFiltrado);\n      } else {\n        let arrFiltrado = productos.filter(\n          (item) => item.data.destacado === true\n        );\n\n        setItems(arrFiltrado);\n      }\n    }\n  };\n\n  useEffect(() => {\n    getProductsFromDB();\n  }, [categoria]);\n\n  return (\n    <>\n      {items.length ? (\n        <ItemList\n          categoria={\n            !!categoria ? `Productos de ${categoria}` : \"Productos destacados\"\n          }\n          productos={items}\n        />\n      ) : (\n        <Loading />\n      )}\n    </>\n  );\n};\n\nexport default ItemListContainer;\n","/home/gaston/Documentos/CoderHouseReact/coder-tienda/src/components/global/Banner/Banner.jsx",["206"],"import { BannerStyle } from \"./Style\";\nimport { useState } from \"react\";\nconst Banner = ({ bannerBoolean }) => {\n  const [slide, setSlide] = useState(bannerBoolean);\n\n  return (\n    <>\n      <BannerStyle>\n        {!slide ? (\n          <div id=\"slide-uno\" className=\"slide-banner\">\n            <h2>\n              <span className=\"spanUno\">Todos los sonidos</span> <br />\n              <span className=\"spanDos\">que estas buscando</span> <br />\n              <span className=\"spanTres\">en un solo lugar!</span>\n            </h2>\n          </div>\n        ) : (\n          <div id=\"slide-dos\" className=\"slide-banner\">\n            <h2>\n              <span className=\"spanDos\">Compre desde casa</span> <br />\n              <span className=\"spanTres\">Rápido... Facil...</span>\n              <span className=\"spanUno\"> Y Seguro</span>\n            </h2>\n          </div>\n        )}\n      </BannerStyle>\n    </>\n  );\n};\nexport default Banner;\n","/home/gaston/Documentos/CoderHouseReact/coder-tienda/src/components/global/Modal/StyledModal.jsx",[],"/home/gaston/Documentos/CoderHouseReact/coder-tienda/src/components/global/ItemListContainer/ItemList.jsx",[],"/home/gaston/Documentos/CoderHouseReact/coder-tienda/src/components/global/CardInfo/CardStyle.jsx",[],"/home/gaston/Documentos/CoderHouseReact/coder-tienda/src/components/global/CardInfo/CardView.jsx",[],"/home/gaston/Documentos/CoderHouseReact/coder-tienda/src/components/global/ItemListContainer/Style.jsx",[],["207","208"],"/home/gaston/Documentos/CoderHouseReact/coder-tienda/src/components/global/AddCart/AddToCart.jsx",[],"/home/gaston/Documentos/CoderHouseReact/coder-tienda/src/components/global/Checkout/CheckoutContainer.jsx",[],"/home/gaston/Documentos/CoderHouseReact/coder-tienda/src/components/global/Checkout/CheckoutStyle.jsx",[],"/home/gaston/Documentos/CoderHouseReact/coder-tienda/src/helpers/cart/getFullPrice.js",[],"/home/gaston/Documentos/CoderHouseReact/coder-tienda/src/components/containers/Cart/CartStyle.jsx",[],"/home/gaston/Documentos/CoderHouseReact/coder-tienda/src/components/containers/Cart/CartView.jsx",["209","210","211","212"],"import { useState } from \"react\";\nimport { CartStyle, Delete } from \"./CartStyle\";\nimport CountContainer from \"../../global/Count/CountContainer\";\n\nconst CartView = ({ data, setData, item, removeItem, cantidad }) => {\n  const [count, setCount] = useState(!!cantidad ? cantidad : 0);\n\n  const handleClickSubst = () => {\n    const newData = {\n      items: [],\n    };\n\n    data.items.map((itemCart) => {\n      if (itemCart.item.id == item.id) {\n        itemCart.cantidad = itemCart.cantidad - 1;\n        setCount(itemCart.cantidad);\n      }\n\n      newData.items.push(itemCart);\n    });\n\n    setData({\n      ...data,\n      newData,\n    });\n  };\n\n  const handleClickAdd = () => {\n    const newData = {\n      items: [],\n    };\n\n    data.items.map((itemCart) => {\n      if (itemCart.item.id == item.id) {\n        itemCart.cantidad = itemCart.cantidad + 1;\n        setCount(count + 1);\n      }\n\n      newData.items.push(itemCart);\n    });\n\n    setData({\n      ...data,\n      newData,\n    });\n  };\n\n  return (\n    <CartStyle>\n      <div className=\"detalle\">\n        <img\n          src={item?.data.imagen || \"https://placehold.it/100x120\"}\n          alt=\"imagen-producto\"\n        />\n        <div className=\"info\">\n          <h2>{item?.data.nombre}</h2>\n          <h3>${item?.data.precio}</h3>\n        </div>\n      </div>\n      <div className=\"count\">\n        <CountContainer\n          max={item?.data?.stock}\n          count={count}\n          handleClickAdd={handleClickAdd}\n          handleClickSubst={handleClickSubst}\n        />\n        {!!item.stock > 1 ? (\n          <span>1 elemento disponible</span>\n        ) : (\n          <span>{item?.data.stock} elementos disponibles</span>\n        )}\n      </div>\n\n      <div className=\"sub-total\">\n        {(item?.data.precio * cantidad).toLocaleString(\"es-AR\", {\n          style: \"currency\",\n          currency: \"ARS\",\n        })}\n      </div>\n      <Delete onClick={() => removeItem(item.id)}>X</Delete>\n    </CartStyle>\n  );\n};\nexport default CartView;\n","/home/gaston/Documentos/CoderHouseReact/coder-tienda/src/components/global/Navbar/CartListNavbar.jsx",["213"],"import { CartList } from \"./Style\";\nimport { Link } from \"react-router-dom\";\nimport getFullPrice from \"../../../helpers/cart/getFullPrice\";\n\nconst CartListNavbar = ({ data }) => {\n  return (\n    <CartList>\n      <span className=\"full-price\">Total: {getFullPrice(data.items)}</span>\n      <Link className=\"to-cart\" to=\"/cart\">\n        Ir al car