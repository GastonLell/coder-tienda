ort {useParams} from 'react-router-dom';\n\nconst ItemListContainer = () => {\n    const [productos, setProductos] = useState([]);\n    const {category} = useParams();\n\n    const getProducts = new Promise((resolve, reject) => {\n\n        if(aProductos) {\n            if(!!category){\n                const productosCategory = aProductos.filter(producto => producto.categoria == category)\n                setTimeout(() => {\n                    resolve(productosCategory);\n                }, 500) \n            } else {\n                setTimeout(()=> {\n                    let destacados = aProductos.filter(producto => producto.destacado === true)\n                    resolve(destacados);\n                }, 500)\n            } \n            \n        } else {\n            reject('No hay productos disponibles...')\n        }\n    })\n    \n\n    useEffect(() => {\n        getProducts\n        .then(rta => {setProductos(rta)})\n        .catch(error => console.log(error))\n    }, [category])\n\n    return(       \n        \n        <>\n            {productos.length ? \n                <ItemList \n                    categoria={!!category ? `Productos de ${category}` : \"Productos destacados\"} \n                    productos={productos} /> : \n                <Loading/>\n            }\n        </>\n    )\n}\n\nexport default ItemListContainer;",["141","142"],"/home/gaston/Documentos/CoderHouseReact/coder-tienda/src/components/global/Cart/CartStyle.jsx",[],"/home/gaston/Documentos/CoderHouseReact/coder-tienda/src/components/global/Cart/Cart.jsx",[],"/home/gaston/Documentos/CoderHouseReact/coder-tienda/src/components/global/Navbar/Style.jsx",[],"/home/gaston/Documentos/CoderHouseReact/coder-tienda/src/components/global/Navbar/ButtonMenu.jsx",[],"/home/gaston/Documentos/CoderHouseReact/coder-tienda/src/components/global/Banner/Banner.jsx",["143"],"import {BannerStyle} from './Style';\nimport {useState} from 'react';\nconst Banner = () => {\n    // terminar slider de banner\n    const [slide, setSlide] = useState(true);\n\n\n    return(\n        <>\n            <BannerStyle>\n            <button>&#10094;</button>\n\n                {\n                    !slide ? \n                    (<div id=\"slide-uno\" className=\"slide-banner\">\n                        <h2>\n                            <span className=\"spanUno\">Todos los sonidos</span> <br/> \n                            <span className=\"spanDos\">que estas buscando</span> <br/>\n                            <span className=\"spanTres\">en un solo lugar!</span>\n                        </h2>\n                    </div>) :\n                    (<div id=\"slide-dos\" className=\"slide-banner\">\n                        <h2>\n                            <span className=\"spanDos\">Compre desde casa</span> <br/> \n                            <span className=\"spanTres\">Rápido... Facil...</span> \n                            <span className=\"spanUno\"> Y Seguro</span>\n                        </h2>\n                    </div> )\n                }\n            <button>&#10095;</button>\n\n            </BannerStyle>\n        </>\n    )\n    \n}\nexport default Banner;","/home/gaston/Documentos/CoderHouseReact/coder-tienda/src/components/Detail/ProductDetail.jsx",[],"/home/gaston/Documentos/CoderHouseReact/coder-tienda/src/components/global/ItemListContainer/Item.jsx",[],"/home/gaston/Documentos/CoderHouseReact/coder-tienda/src/components/global/Count/CountContainer.jsx",["144","145","146","147"],"import { useContext, useState, useEffect } from \"react\";\nimport Count from \"./Count\";\nimport { Store } from \"../../../store/CartContext\";\n\nconst CountContainer = ({ max, count, addCount, substCount }) => {\n  return (\n    <Count\n      handleClickSubst={substCount}\n      handleClickAdd={addCount}\n      count={count}\n      max={max}\n    />\n  );\n};\n\nexport default CountContainer;\n","/home/gaston/Documentos/CoderHouseReact/coder-tienda/src/components/global/Banner/Style.jsx",[],"/home/gaston/Documentos/CoderHouseReact/coder-tienda/src/components/global/Count/Count.jsx",[],"/home/gaston/Documentos/CoderHouseReact/coder-tienda/src/components/global/Count/CountStyled.jsx",["148"],"import styled from 'styled-components';\nimport {Colores, Fuentes} from '../../utils/Constants';\nimport {ModalStyled} from '../Modal/StyledModal';\n\nexport const CountStyle = styled.div`\n    display: flex;\n    align-items: center;\n    font-family: ${Fuentes.fuenteUno};\n    margin-bottom: 10px;\n    border: 1px solid ${Colores.colorCuatro};\n    width: 90px;\n    max-height: 40px;\n    span{\n        width: 30px;\n        height: 40px;\n        text-align: center;\n        padding-top: 8px;\n        font-size: 20px;        \n    }\n    button{\n        width: 30px;\n        height: 40px;\n        border: none;\n        font-size: 20px;\n        background-color: transparent;\n    }\n`\n","/home/gaston/Documentos/CoderHouseReact/coder-tienda/src/store/CartContext.js",[],"/home/gaston/Documentos/CoderHouseReact/coder-tienda/src/components/global/AddCart/AddCartContainer.jsx",[],"/home/gaston/Documentos/CoderHouseReact/coder-tienda/src/components/Detail/DetailContainer.jsx",["149","150"],"import ProductDetail from \"./ProductDetail\";\nimport aProducos from \"../../assets/imagenes/aProductos\";\nimport { useEffect, useState, useContext } from \"react\";\nimport { useParams, useHistory } from \"react-router-dom\";\nimport { Store } from \"../../store/CartContext\";\n\nconst DetailContainer = () => {\n  //tomo el id del producto para mostrar\n  const { idProducto } = useParams();\n\n  // estado para guardar el items y mostrar\n  const [item, setItem] = useState({});\n\n  //estado global de la app\n  const [data] = useContext(Store);\n\n  //para redireccionar la pagina sin recargar\n  const history = useHistory();\n\n  //funcion para redireccionar a cart\n  const handleRedirect = () => {\n    history.push(\"/cart\");\n  };\n\n  //promesa buscando producto en array de productos y guardo en el estado de items\n  const getItems = new Promise((resolve, reject) => {\n    if (!!aProducos) {\n      setTimeout(() => {\n        const productoSeleccionado = aProducos.find(\n          (producto) => producto.idProducto == idProducto\n        );\n\n        resolve(productoSeleccionado);\n      }, 100);\n    } else {\n      reject(\"No hay productos disponibles...\");\n    }\n  });\n\n  useEffect(() => {\n    getItems\n      .then((res) => {\n        const cartItem = data.items.find(\n          ({ idProducto }) => idProducto === res.idProducto\n        );\n        setItem({ ...res, amount: cartItem ? cartItem.amount : 1 });\n      })\n      .catch((err) => console.log(err));\n  }, []);\n\n  return <ProductDetail item={item} handleRedirect={handleRedirect} />;\n};\nexport default DetailContainer;\n","/home/gaston/Documentos/CoderHouseReact/coder-tienda/src/components/global/ItemListContainer/ItemList.jsx",[],"/home/gaston/Documentos/CoderHouseReact/coder-tienda/src/components/global/Loading/Loading.jsx",[],"/home/gaston/Documentos/CoderHouseReact/coder-tienda/src/components/Detail/DetailStyle.jsx",[],"/home/gaston/Documentos/CoderHouseReact/coder-tienda/src/components/utils/Constants.jsx",[],"/home/gaston/Documentos/CoderHouseReact/coder-tienda/src/components/global/Navbar/CartWidget.jsx",["151"],"import { Cart, Contador } from \"./Style\";\nimport { MdShoppingCart } from \"react-icons/md\";\nimport { Store } from \"../../../store/CartContext\";\nimport { useContext } from \"react\";\nimport getItemsAmount from \"../../../helpers/cart/getItemsAmount\";\n\nconst CartWidget = ({ openClosedCart }) => {\n  const [data, setData] = useContext(Store);\n  return (\n    <Cart onClick={openClosedCart}>\n      <MdShoppingCart />\n      <Contador>{getItemsAmount(data.items)} </Contador>\n    </Cart>\n  );\n};\nexport default CartWidget;\n","/home/gaston/Documentos/CoderHouseReact/coder-tienda/src/helpers/cart/getItemsAmount.js",[],"/home/gaston