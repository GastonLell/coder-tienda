ainer;\n","/home/gaston/Documentos/CoderHouseReact/coder-tienda/src/db/config.js",[],["131","132"],"/home/gaston/Documentos/CoderHouseReact/coder-tienda/src/components/global/Navbar/Style.jsx",[],"/home/gaston/Documentos/CoderHouseReact/coder-tienda/src/components/global/Navbar/ButtonMenu.jsx",[],"/home/gaston/Documentos/CoderHouseReact/coder-tienda/src/components/global/Footer/FooterStyle.jsx",[],"/home/gaston/Documentos/CoderHouseReact/coder-tienda/src/components/global/Cart/CartStyle.jsx",[],"/home/gaston/Documentos/CoderHouseReact/coder-tienda/src/components/global/Cart/Cart.jsx",["133","134","135","136"],"import { useState } from \"react\";\nimport { CartStyle, Delete } from \"./CartStyle\";\nimport CountContainer from \"../Count/CountContainer\";\n\nconst Cart = ({ data, setData, item, removeItem, cantidad }) => {\n  const [count, setCount] = useState(!!cantidad ? cantidad : 0);\n\n  const handleClickSubst = () => {\n    const newData = {\n      items: [],\n    };\n\n    data.items.map((itemCart) => {\n      if (itemCart.item.id == item.id) {\n        itemCart.cantidad = itemCart.cantidad - 1;\n        setCount(itemCart.cantidad);\n      }\n\n      newData.items.push(itemCart);\n    });\n\n    setData({\n      ...data,\n      newData,\n    });\n  };\n\n  const handleClickAdd = () => {\n    const newData = {\n      items: [],\n    };\n\n    data.items.map((itemCart) => {\n      if (itemCart.item.id == item.id) {\n        itemCart.cantidad = itemCart.cantidad + 1;\n        setCount(count + 1);\n      }\n\n      newData.items.push(itemCart);\n    });\n\n    setData({\n      ...data,\n      newData,\n    });\n  };\n\n  return (\n    <CartStyle>\n      <div className=\"detalle\">\n        <img\n          src={item?.data.imagen || \"https://placehold.it/100x120\"}\n          alt=\"imagen-producto\"\n        />\n        <div className=\"info\">\n          <h2>{item?.data.nombre}</h2>\n          <h3>${item?.data.precio}</h3>\n        </div>\n      </div>\n      <div c